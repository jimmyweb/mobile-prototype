.book {
	display: block;
	float: left;
	margin: 10px;
	width: 300px;
	height: 100px;
	color: #fff;
	position: relative;
	z-index: 0;
	background-position: center center;
	background-repeat: no-repeat;
	background-size: cover;

	&.loading {
		.loader {
			opacity: 1;
		}
	}


	.loader {
        position: absolute;
        top: 0;
        bottom: 0;
        left: 0;
        right: 0;
        background: rgba(#fff, 0.85);
        z-index: 150;
        opacity: 0;
        @include transition(all 0.2s ease-in-out);

        span {
        	position: absolute;
        	top: 0;
        	left: 0;
        	right: 0;
        	bottom: 0;
        	background: url(../../img/logo-gray.png) center center no-repeat;
        	background-size: cover;
        	width: 20px;
        	height: 20px;
        	display: block;
        	margin: auto;
        	-webkit-animation:spin 1s linear infinite;
		    -moz-animation:spin 1s linear infinite;
		    animation:spin 1s linear infinite;
        }
    }

	.title {
		font-size: 1.5em;
		font-family: $proximaB;
		font-weight: bold;
		margin-top: 5px;
	}

	.text {
		font-family: $proxima;
		font-size: 1.4em;
		font-weight: 100;
		margin: 10px 10px 0 0;
	}

	.meta {
		position: absolute;
		bottom: 5px;
		right: 15px;
		font-size: 1.3em;
	}

	&:before {
		content: "";
		display: block;
		position: absolute;
		right: -2px;
		top: 1px;
		width: 98%;
		height: 100%;
		background: transparent;
		border: 1px solid rgba(255,255,255,0.7);
		border-top: 0;
		border-left: 0;
		z-index: -2;
	}

	&:after {
		content: "";
		display: block;
		z-index: -1;
		position: absolute;
		right: -4px;
		top: 4px;
		width: 98%;
		height: 100%;
		background: transparent;
		border: 1px solid rgba(255,255,255,0.7);
		border-top: 0;
		border-left: 0;
	}

	.star {
		background: url(../../img/single-star.png) center center no-repeat;
		background-size: cover;
		width: 13px;
		height: 13px;
		position: absolute;
		right: 10px;
		bottom: 20px;
	}

	.badge {
		display: block;
		position: absolute;
		top: -5px;
		right: -5px;
		margin: 0;
		padding: 0;
		background: #fff;
		color: #2bbed8;
		width: 26px;
		height: 26px;
		line-height: 26px;
		font-size: 13px;
		border-radius: 50%;
		text-align: center;
	}

	.cross {
		display: none;
		width: 26px;
		height: 26px;
		position: absolute;
		bottom: 10px;
		right: 5px;

		&:before {
			content: "";
			position: absolute;
			left: 0;
			top: 50%;
			margin-top: -1px;
			height: 2px;
			width: 100%;
			background: #fff;
		}

		&:after {
			content: "";
			position: absolute;
			top: 0;
			left: 50%;
			margin-left: -1px;
			width: 2px;
			height: 100%;
			background: #fff;
		}
	}

	.content {
		margin-left: 10%;
		text-align: left;
		padding: 15px 0;
	}

	.left-border {
		position: absolute;
		top: 0;
		left: 0;
		height: 100%;
		width: 10px;
		background: rgba(0,0,0,0.2);

		&:after {
			content: "";
			position: absolute;
			bottom: -6px;
			left: 3px;
			height: 6px;
			width: 100%;
			background: #555;
			-moz-transform: scale(1) rotate(0deg) translateX(0px) translateY(0px) skewX(45deg) skewY(0deg);
			-webkit-transform: scale(1) rotate(0deg) translateX(0px) translateY(0px) skewX(45deg) skewY(0deg);
			-o-transform: scale(1) rotate(0deg) translateX(0px) translateY(0px) skewX(45deg) skewY(0deg);
			-ms-transform: scale(1) rotate(0deg) translateX(0px) translateY(0px) skewX(45deg) skewY(0deg);
			transform: scale(1) rotate(0deg) translateX(0px) translateY(0px) skewX(45deg) skewY(0deg);
		}
	}

	&.common {
		height: auto;
	}

	&.black {
		.left-border:after {
			background: #000;
		}
	}	

	&.button {
		background: #a4a8b3;
		position: relative;
		bottom: auto;
		right: auto;
		text-indent: 0;

		.content {
			padding-top: 40px;
		}

		.cross {
			display: block;
		}

		.left-border {
			background: #fff;

			&:after {
				background: #ccc;
			}
		}
	}
}

