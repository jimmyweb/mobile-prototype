	.homescreen {
		-webkit-transform: scale(3);
		transform: scale(3);
		opacity: 0;
	}
}

.backAnimation {
	.homescreen {
		@include transition(all 0.5s $iosAnimation 0.1s);
	}
}

.homescreen {
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	background: transparent;
	overflow: hidden;
	opacity: 1;
	@include transition(all 0.5s $iosAnimation);

	nav {
		background: rgba(#fff, 0.5);
		text-align: center;
		height: 40px;
		position: fixed;
		top: 0;
		width: 100%;
		z-index: 1000;
		-webkit-box-shadow: 0px 3px 10px 3px rgba(0, 0, 0, 0.3);
		box-shadow: 0px 3px 10px 3px rgba(0, 0, 0, 0.3);

		&:before {
			content: "";
			position: absolute;
			top: 12px;
			left: 10px;
			height: 15px;
			width: 60px;
			background: url(../../img/logo.png) center top no-repeat;
			background-size: contain;
		}

		h1 {
			text-align: center;
			font-size: 1.6em;
			font-family: $proxima;
			color: #000;
			margin: 10px 50px 0 50px;
			font-weight: 100;
		}

		ul {
			list-style: none;
			display: inline-block;
			padding: 0;
			margin: 0;
			@extend .clearfix;

			li {
				display: block;
				float: left;
			}

			a {
				display: block;
				text-decoration: none;
				text-align: center;
				color: #fff;
				font-weight: 100;
				font-size: 1.7em;
				font-family: $proxima;
				width: 100px;
				height: 30px;
				line-height: 30px;
				border: 1px solid #fff;

				&:nth-child(1){
					margin-left: -1px;
				}

				&.active {
					background: rgba(#fff, 0.3);
				}
			}
		}

		#search-button {
			display: block;
			position: absolute;
			top: 0;
			right: 0;
			width: 35px;
			height: 40px;
			background: url(../../img/search-icon.png) center center no-repeat;
			background-size: 20px 20px;
		}
	}

	.card {
		background: url(../../img/home-bg.png) center center no-repeat;
		background-size: cover;
		top: 40px;

		.badge {
			left: auto;
		}

		&.shadow {
             -webkit-box-shadow:  0px 0px 20px 2px rgba(0, 0, 0, 0.5);
        
            box-shadow:  0px 0px 20px 2px rgba(0, 0, 0, 0.5);     
    	}
	}

	.apps {
		list-style: none;
		padding: 0;
		margin: 0;

		li {
			display: inline-block;
		}
		a {
			display: block;
			width: 30px;
		}

		img {
			max-width: 100%;
		}
	}

	.home-shelf {

		.book {
			&:nth-child(1) {
				height: 57%;
				margin: 10px 3% 0 3%;
				clear: both;
			}

			&:nth-child(2) {
				height: 28%;
				float: left;
				width: 45%;
				margin: 15px 2% 0 3%;
			}

			&:nth-child(3) {
				height: 28%;
				float: left;
				width: 45%;
				margin: 15px 3% 0 2%;
			}

			.content {
				position: absolute;
				left: 0;
				padding: 0 10px;
				box-sizing: border-box;
				bottom: 10%;
				width: 100%;
				margin-left: 10px;
			}

			&.button {
				.cross {
					right: auto;
					left: 25px;
				}

				.text {

				}
			}
		}
	}

	.interest-shelf {
		.book {
			&:nth-child(1) {
				height: 43%;
				width: 45%;
				margin: 15px 2% 0 3%;
				float: left;
			}

			&:nth-child(2) {
				height: 43%;
				float: left;
				width: 45%;
				margin: 15px 3% 0 2%;
			}

			&:nth-child(3) {
				height: 43%;
				float: left;
				width: 45%;
				margin: 15px 2% 0 3%;
				clear: left;
			}

			&:nth-child(4) {
				height: 43%;
				float: left;
				width: 45%;
				margin: 15px 3% 0 2%;
			}

			.content {
				position: absolute;
				left: 0;
				padding: 0 10px;
				box-sizing: border-box;
				bottom: 25%;
				width: 100%;
				margin-left: 10px;
			}

			&.button {
				.cross {
					right: auto;
					left: 25px;
				}

				.text {

				}
			}
		}
	}

	.magazine-shelf {
		.book {
			&:nth-child(1) {
				height: 44%;
				margin: 10px 3% 0 3%;
				clear: both;

				.content {
					bottom: 15%;
					text-align: center;
				}

				.avatar {
					width: 100px;
				}
			}

			&:nth-child(2) {
				height: 43%;
				float: left;
				width: 45%;
				margin: 15px 2% 0 3%;
			}

			&:nth-child(3) {
				height: 43%;
				float: left;
				width: 45%;
				margin: 15px 3% 0 2%;
			}

			.content {
