* {
    -webkit-touch-callout:none;
    -webkit-user-select:none;
    -khtml-user-select:none;
    -moz-user-select:none;
    -ms-user-select:none;
    user-select:none;
}

.stream-on {
    .wrapper {
        -webkit-transform: scale(1);
        transform: scale(1);
    }
}

.backAnimation {
    .wrapper {
        @include transition(all 0.5s $iosAnimation);
    }
}

.wrapper {
	width: 100%;
    height: 100%;
	position: absolute;
	top: 0;
	left: 0; 
    margin: auto;
    overflow: hidden;
    -webkit-perspective: 1000px;
    perspective: 1000px;
    -webkit-transform: scale(0);
    transform: scale(0);
    @include transition(all 0.5s $iosAnimation 0.1s);
}

#tapper {
    width: 90px;
    height: 90px;
    border-radius: 50%;
    background: rgba(255,255,255,0.4);
    border: 3px solid rgba(0,0,0,0.4);
    position: absolute;
    left: -999px;
    top: -999px;
    opacity: 0;
    z-index: 9999;
    
    > div {
        position: absolute;
        top: 0;
        bottom: 0;
        left: 0;
        right: 0;
        margin: auto;
        width: 50px;
        height: 50px;
        border-radius: 50%;
        background: rgba(255, 255, 255, 0.9);
        -webkit-box-shadow:  0px 5px 10px -5px rgba(0, 0, 0, .3);
        box-shadow:  0px 5px 10px -5px rgba(0, 0, 0, .3);
        text-align: center;
        line-height: 50px;
        border: 2px solid tomato;
        opacity: 0;
        @include translate3d(0, 0, 0);
        @include transition(all 0.2s ease-in-out);
    }

    &.tl,
    &.tr,
    &.bl,
    &.br,
    &.tc,
    &.bc,
    &.cc,
    &.cl,
    &.cr {
        opacity: 1;

        > div {
            opacity: 1;
        }
    }

    &.bc, &.cc {
        .opt1 {
            @include translate3d(-60px, -80px, 0);
        }

        .opt2 { 
            @include translate3d(0px, -100px, 0);
        }

        .opt3 {
            @include translate3d(60px, -80px, 0);
        }
    }

    &.tc {
        .opt1 {
            @include translate3d(-60px, 80px, 0);
        }

        .opt2 { 
            @include translate3d(0px, 100px, 0);
        }

        .opt3 {
            @include translate3d(60px, 80px, 0);
        }
    }

    &.tl {
        .opt1 {
            @include translate3d(100px, 0px, 0);
        }

        .opt2 { 
            @include translate3d(80px, 60px, 0);
        }

        .opt3 {
            @include translate3d(30px, 100px, 0);
        }
    }

    &.tr {
        .opt1 {
            @include translate3d(-100px, 0px, 0);
        }

        .opt2 { 
            @include translate3d(-80px, 60px, 0);
        }

        .opt3 {
            @include translate3d(-30px, 100px, 0);
        }
    }

    &.bl, &.cl {
        .opt3 {
            @include translate3d(100px, 0px, 0);
        }

        .opt2 { 
            @include translate3d(80px, -60px, 0);
        }

        .opt1 {
            @include translate3d(30px, -100px, 0);
        }
    }

    &.br, &.cr {
        .opt3 {
            @include translate3d(-100px, 0px, 0);
        }

        .opt2 { 
            @include translate3d(-80px, -60px, 0);
        }

        .opt1 {
            @include translate3d(-30px, -100px, 0);
        }
    }
}

