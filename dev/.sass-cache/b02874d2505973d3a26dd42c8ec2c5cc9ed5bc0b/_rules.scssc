.card {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;

    &.card-3d {
        -webkit-transform-style: preserve-3d;
        transform-style: preserve-3d;
        -webkit-transform: rotateY(0deg) scale(1) translateZ(-160px);
        transform: rotateY(0deg) scale(1) translateZ(-160px);
        -webkit-backface-visibility: hidden;
        -webkit-transform-origin: 50% 50%;
        transform-origin: 50% 50%;

        .card-content {
            -webkit-backface-visibility: hidden;
            backface-visibility: hidden;
            -webkit-transform: translateZ(160px);
            transform: translateZ(160px);
            -webkit-transform-origin: 0 0;
            transform-origin: 0 0;
        }

        .details {
            -webkit-backface-visibility: hidden;
            backface-visibility: hidden;
            -webkit-transform-origin: 50% 50%;
            transform-origin: 50% 50%;
            -webkit-transform: rotateY(90deg) translateZ(160px);
            transform: rotateY(90deg) translateZ(160px);
        }
    }

    &.card-transition {
        .card-content {
            -webkit-transition: all 800ms ease-in-out;
            transition: all 800ms ease-in-out;
        }

        .details {
            -webkit-transition: all 800ms ease-in-out;
            transition: all 800ms ease-in-out;
        }
    }

    &.detailsOn {
        -webkit-transform: rotateY(-90deg) translateZ(0px) translateX(-50%);
        transform: rotateY(-90deg) translateZ(0px) translateX(-50%);
    }

