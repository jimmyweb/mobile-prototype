        font-size: 12px;

        &.blue {
            background: #3fb4ff url(../../img/blue-badge.png) 50% 10px no-repeat;
            background-size: auto 30px;
        }

        &.green {
            background: #93d168 url(../../img/green-badge.png) 50% 10px no-repeat;
            background-size: auto 30px;
        }
    }
}

.screenshot-slider{
    overflow: hidden;
    position: relative;
    margin-left: 30px;

}

.screenshot-wrap {
    overflow: hidden;
    position: relative;
    height: 100%;
    @extend .clearfix;
}

.screenshot-wrap > div {
    float:left;
    width:100%;
    height: 100%;
    position: relative;
    background-size: cover;
    background-position: center center;
    background-repeat: no-repeat;
    background-color: #fff;
    opacity: 0.99;
    -webkit-backface-visibility: hidden;
    -webkit-perspective: 1000;
}

.bullets {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    text-align: center;
    height: 17px;
    z-index: 999;

    > div {
        border-radius: 15px 15px 0 0;
        background: #fff;
        display: inline-block;
        padding: 5px 10px;
        @extend .clearfix;

        i {
            display: block;
            float: left;
            margin: 0 3px;
            border-radius: 50%;
            background: #dadada;
            width: 7px;
            height: 7px;

            &.active {
                background: #000;
            }
        }
    }
}

.slide-title{
    font-weight: 100;
    font-size: 16px;
    margin: 20px 20px 10px 40px;
                
}

#content-list {
    background: rgba(0, 0, 0, 0.85);
    -webkit-box-shadow:  0px 0px 6px 0px rgba(0, 0, 0, 0.2);
    box-shadow:  0px 0px 6px 0px rgba(0, 0, 0, 0.2);
    position: absolute;
    z-index: 999;
    padding: 0 5px;
    height: 30px;
    line-height: 28px;
    font-weight: 100;
    top: 65px;
    right: 12px;
    max-width: 40%;
    opacity: 0;
    color: #fff;
    font-size: 12px;
    @include translate3d(130%, 0, 0);
    @include transition(all 0.3s ease-in-out);

    &.active {
        
        opacity: 1;
        @include translate3d(0, 0, 0);
        &.scrolled {
            @include transition(none);
        }
    }

    &:after {
        content: "";
        position: absolute;
        right: -10px;
        top: 0;
        width: 0px;
        height: 0px;
        border-style: solid;
        border-width: 16px 0 16px 10px;
        border-color: transparent transparent transparent rgba(0,0,0,0.85);
    }
    
}

.navigation {
    background: url(../../img/navi.png) 10px 0 no-repeat;
    background-size: 25px auto;
    padding-bottom: 10px;

    > div{
        margin-left: 20px;
    }

    a {
        border: 1px solid $seledyn;
        color: $seledyn;
        height: 30px;
        line-height: 30px;
        font-size: 12px;
        text-align: center;
        display: inline-block;
        text-decoration: none;
        padding: 0 10px;
        margin: 5px;
        @include transition(all 0.1s ease-in-out);

        &:active{
            background: $seledyn;
            color: #fff;
        }
    }
}

.apps-list {
    list-style: none;
    padding: 0;
    float: left;
    width: 45%;
    margin-left: 10%;

    &:last-child{
        margin-left: 2%;
        margin-right: 1%;
        width: 42%;
    }
    li {
        margin: 5px 0;
        @extend .clearfix;
        
        a {
            text-decoration: none;
            color: #333;
        }

        div {
            margin-left: 55px;
        }

        img {
            width: 50px;
            height: auto;
            float: left;
        }

        h3,
        span {
            font-size: 10px;
        }

        span {
            color: #ccc;
        }
    }
}

.reviews {
    background: url(../../img/reviews.png) 10px 20px no-repeat;
    background-size: 25px auto;
}

.price-history {
    background: url(../../img/prices.png) 10px 20px no-repeat;
    background-size: 25px auto;
}

.downloads{
    background: url(../../img/downloads-black.png) 10px 20px no-repeat;
    background-size: 25px auto;
}

.release-notes{
    background: url(../../img/update.png) 10px 25px no-repeat;
    background-size: 25px auto;
    ul {
        padding-left: 60px;
    }
    li {
        font-size: 12px;
    }
}

.detailed-description {
    background-image: url(../../img/app-icon.png);
    background-repeat: no-repeat;
    background-position: 10px 20px;
    background-size: 25px auto;

    p {
        font-size: 12px;
        margin: 8px 30px 8px 40px;
    }
    
}

.developer-info {
    background-image: url(../../img/supercell.png);
    background-repeat: no-repeat;
    background-position: 10px 20px;
    background-size: 25px auto;

    p {
        font-size: 12px;
        margin: 8px 30px 8px 40px;
    }
    
}

.metacritic {
    position: absolute;
    background: rgba(#93d168, 0.6);
    bottom: 150px;
    right: 0;
    margin-left: 30px;
    padding-left: 45px;
    padding-right: 5px;
    height: 90px;
    @extend .clearfix;
    
    &:before {
        content: "";
        display: block;
        width: 90px;
        height: 90px;
        background: url(../../img/news.png) center center no-repeat;
        background-size: cover;
        position: absolute;
        left: -50px;
        top: 0;
    }

    p {
        color: #fff;
        font-size: 1.2em;
        font-weight: 100;
        margin: 5px 0 0 0;
        float: left;
    }
    .meta {
        margin-top: 10px;
        @extend .clearfix;
    }

    .author {
        color: #fff;
        font-size: 1em;
        font-weight: 500;
        float: left;
        width: 50%;
    }

    .score {
        color: #fff;
        font-size: 1em;
        font-weight: 500;
        float: right;

        i {
            display: inline-block;
            color: #000;
            border: 3px solid #ffde00;
            width: 15px;
            height: 15px;
            line-height: 15px;
            background: #fff;
            text-align: center;
            font-weight: bold;
            font-style: normal;
        }
    }
}

.btn{ 
    position: absolute;
    bottom: 10px;
    right: 10px;
    padding: 5px 35px;
    border: 2px solid #000;
    text-decoration: none;
    color: #000;
    font-size: 1.6em;

    &.blue {
        background: $seledyn;
        color:#fff;
        border: 0;
    }
}

#cards-navigation {
    position: fixed;
    top: 0;
    left: 0;
    height: 40px;
    background: #fff;
    z-index: 1000;
    width: 100%;
    box-sizing: border-box;
    -webkit-box-shadow:  0px 3px 10px 3px rgba(0, 0, 0, 0.5);
    box-shadow:  0px 3px 10px 3px rgba(0, 0, 0, 0.5);

    h2 {
        text-align: center;
        font-family: $proxima;
        font-weight: 100;
        line-height: 40px;
    }

    a {
        position: absolute;
        top: 0px;
        left: 0px;
        width: 40px;
        height: 100%;

        &:active {
            background: rgba(255,255,255, 0.2);
        }
        span {
            display: block;
            position: absolute;
            top: 0;
            bottom: 0;
            left: 0;
            right: 0;
            margin: auto;
            width: 12px;
            height: 12px;
            border-top: 1px solid #000;
            border-left: 1px solid #000;
            -webkit-transform: rotate(-45deg);
            transform: rotate(-45deg);
        }
    }
}



