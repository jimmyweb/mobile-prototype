:@template"E.stream-on {
	.homescreen {
		-webkit-transform: scale(3);
		transform: scale(3);
		opacity: 0;
	}
}

.backAnimation {
	.homescreen {
		@include transition(all 0.5s $iosAnimation 0.1s);
	}
}

.homescreen {
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	/*background: url(../../img/home-bg.png) center center no-repeat;
	background-size: cover;*/
	background: transparent;
	overflow: hidden;
	opacity: 1;
	@include transition(all 0.5s $iosAnimation);
	/*-webkit-transition: all 0.5 cubic-bezier(0.23,1,0.32,1);
	transition: all 0.5 cubic-bezier(0.23,1,0.32,1);*/

	

	nav {
		
		/*background: url(../../img/home-bg.png) center top no-repeat;*/
		background-size: cover;
		text-align: center;
		padding: 50px 0 10px 0;
		position: fixed;
		top: 0;
		width: 100%;
		z-index: 1000;

		&:before {
			content: "";
			position: absolute;
			top: 15px;
			left: 50%;
			margin-left: -40px;
			height: 20px;
			width: 80px;
			background: url(../../img/logo.png) center top no-repeat;
			background-size: auto 20px;
		}

		ul {
			list-style: none;
			display: inline-block;
			padding: 0;
			margin: 0;
			@extend .clearfix;

			li {
				display: block;
				float: left;
			}

			a {
				display: block;
				text-decoration: none;
				text-align: center;
				color: #fff;
				font-weight: 100;
				font-size: 1.7em;
				font-family: $proxima;
				width: 100px;
				height: 30px;
				line-height: 30px;
				border: 1px solid #fff;

				&:nth-child(1){
					margin-left: -1px;
				}

				&.active {
					background: rgba(#fff, 0.3);
				}
			}
		}

		#search-button {
			display: block;
			position: absolute;
			top: 0;
			right: 0;
			width: 35px;
			height: 40px;
			background: url(../../img/search-icon.png) center center no-repeat;
			background-size: 20px 20px;
		}
	}

	.scrolling-content {
		position: absolute;
		top: 100px;
		bottom: 0;
		left: 0;
		right: 0;
		margin: auto;
		-webkit-backface-visibility: hidden;
		backface-visibility: hidden;
		@include transition(all 0.6s $iosAnimation);

		> div {
			-webkit-backface-visibility: hidden;
			backface-visibility: hidden;
		}

		&.my-shelf {
			left: 100%;
			width: 100%;
			opacity: 0;

			.book {
				float: none;
				min-height: 70px;
				height: auto;
				display: table;

				.avatar {
					width: 40px;
					height: auto;
					border-radius: 50%;
					display: inline-block;
				}

				.title {
					display: inline-block;
					margin-left: 20px;
					margin-right: 120px;
				}

				.cross {
					bottom: auto;
					top: 50%;
					margin-top: -13px;
				}

				&.button {
					.content {
						padding: 5px 0;
					}

					.title {
						margin-right: 50px;
					}
				}
				.content {
					display: table-cell;
					vertical-align: middle;
					margin-right: 120px;

					&.with-avatar {
						.avatar {
							position: absolute;
							top: 50%;
							margin-top: -20px;
							left: 20px;
						}

						.title {
							margin-left: 70px;
						}
					}
				}

				.apps {
					list-style: none;
					padding: 0;
					margin: 0;
					position: absolute;
					top: 10px;
					right: 5px;
					width: 100px;

					li {
						display: inline-block;
					}
					a {
						display: block;
						width: 20px;
					}

					img {
						max-width: 100%;
					}
				}
			}
		}
	}

	&.switched {

		.scrolling-content {
			&.my-shelf {
				-webkit-transform: translate3d(-100%, 0, 0);
				transform: translate3d(-100%, 0, 0);
				opacity: 1;
			}

			&.home-shelf {
				-webkit-transform: translate3d(-100%, 0, 0);
				transform: translate3d(-100%, 0, 0);
				opacity: 0;
			}
		}
		
	}

	.horizontal-content {
		height: 170px;
		overflow-y: auto;
		-webkit-backface-visibility: hidden;
		backface-visibility: hidden;
		> div {
			-webkit-backface-visibility: hidden;
			backface-visibility: hidden;
			width: 1000em;
			@extend .clearfix;
			padding: 0;
			margin: 0 0 0 10px;
			.book {
				float: left;
				height: 160px;
				width:270px;
				margin: 0 10px 0 0;

				.app-icon {
					width: 60px;
					height: auto;
					position: absolute;
					top: 10px;
					right: 10px;
				}

				.title {
					margin-right: 80px;
				}

				&.article {

					.content {
						position: absolute;
						top: 0;
						bottom: 0;
						left: 10px;
						right: 0;
						box-sizing: border-box;
						padding-left: 5px;
						margin-left: 0;
						background: rgba(0, 0, 0, 0.3);
					}

					.left-border {
						background: rgba(0, 0, 0, 0.5);
					}

					.title {
						position: absolute;
						bottom: 30px;
						left: 0;
						width: 100%;
						box-sizing: border-box;
						padding: 0 10px 0 10px;
					}
				}
			}
		}
	}

	.vertical-content {
		@extend .clearfix;
		.col {
			width: 50%;
			float: left;
			margin-bottom: 20px;

			&:first-child {
				.book {
					margin-right: 5px;
				}
			}

			&:last-child {
				.book {
					margin-left: 5px;
				}
			}
		}

		.book {
			width: auto;
			height: auto;
			margin: 10px 10px 20px 10px;
			float: none;

			.text {
				margin-top: 5px;
			}

			.apps {
				list-style: none;
				padding: 0;
				margin: 15px 0;

				li {
					display: inline-block;
				}

				a {
					display: block;
					width: 20px;
				}
			}

			&.tall {
				.title {
					margin-top: 80px;
				}
			}
		}
	}
}

.book {
	display: block;
	float: left;
	margin: 10px;
	width: 300px;
	height: 100px;
	color: #fff;
	position: relative;
	z-index: 0;
	background-position: center center;
	background-repeat: no-repeat;
	background-size: cover;

	&.loading {
		.loader {
			opacity: 1;
		}
	}


	.loader {
        position: absolute;
        top: 0;
        bottom: 0;
        left: 0;
        right: 0;
        /*background: rgba(#fff, 0.9) url(../../img/ajax-loader.gif) center center no-repeat;*/
        background: rgba(#fff, 0.95);
        z-index: 150;
        opacity: 0;
        @include transition(all 0.1s ease-in-out);
        span {
        	position: absolute;
        	top: 0;
        	left: 0;
        	right: 0;
        	bottom: 0;
        	background: url(../../img/logo-gray.png) center center no-repeat;
        	background-size: cover;
        	width: 20px;
        	height: 20px;
        	display: block;
        	margin: auto;
        	-webkit-animation:spin 1s linear infinite;
		    -moz-animation:spin 1s linear infinite;
		    animation:spin 1s linear infinite;
        }
    }

	.title {
		font-size: 1.5em;
		font-family: $proximaB;
		font-weight: bold;
		margin-top: 5px;
	}

	.text {
		font-size: 1.5em;
		font-weight: 100;
		margin: 10px 10px 0 0;
	}

	.meta {
		position: absolute;
		bottom: 5px;
		right: 15px;
		font-size: 1.3em;
	}

	&:before {
		content: "";
		display: block;
		position: absolute;
		right: -2px;
		top: 1px;
		width: 98%;
		height: 100%;
		background: transparent;
		border: 1px solid rgba(255,255,255,0.7);
		border-top: 0;
		border-left: 0;
		z-index: -2;
	}

	&:after {
		content: "";
		display: block;
		z-index: -1;
		position: absolute;
		right: -4px;
		top: 4px;
		width: 98%;
		height: 100%;
		background: transparent;
		border: 1px solid rgba(255,255,255,0.7);
		border-top: 0;
		border-left: 0;
	}

	.star {
		background: url(../../img/single-star.png) center center no-repeat;
		background-size: cover;
		width: 13px;
		height: 13px;
		position: absolute;
		right: 10px;
		bottom: 20px;
	}

	.badge {
		display: block;
		position: absolute;
		top: -5px;
		right: -5px;
		margin: 0;
		padding: 0;
		background: #fff;
		color: #2bbed8;
		width: 26px;
		height: 26px;
		line-height: 26px;
		font-size: 13px;
		border-radius: 50%;
		text-align: center;
	}

	.cross {
		display: none;
		width: 26px;
		height: 26px;
		position: absolute;
		bottom: 10px;
		right: 5px;

		&:before {
			content: "";
			position: absolute;
			left: 0;
			top: 50%;
			margin-top: -1px;
			height: 2px;
			width: 100%;
			background: #fff;
		}

		&:after {
			content: "";
			position: absolute;
			top: 0;
			left: 50%;
			margin-left: -1px;
			width: 2px;
			height: 100%;
			background: #fff;
		}
	}

	.content {
		margin-left: 10%;
		text-align: left;
		padding: 5px 0;
	}

	.left-border {
		position: absolute;
		top: 0;
		left: 0;
		height: 100%;
		width: 10px;
		background: rgba(0,0,0,0.2);

		&:after {
			content: "";
			position: absolute;
			bottom: -6px;
			left: 3px;
			height: 6px;
			width: 100%;
			background: #555;
			-moz-transform: scale(1) rotate(0deg) translateX(0px) translateY(0px) skewX(45deg) skewY(0deg);
			-webkit-transform: scale(1) rotate(0deg) translateX(0px) translateY(0px) skewX(45deg) skewY(0deg);
			-o-transform: scale(1) rotate(0deg) translateX(0px) translateY(0px) skewX(45deg) skewY(0deg);
			-ms-transform: scale(1) rotate(0deg) translateX(0px) translateY(0px) skewX(45deg) skewY(0deg);
			transform: scale(1) rotate(0deg) translateX(0px) translateY(0px) skewX(45deg) skewY(0deg);
		}
	}

	&.fire {
		background: rgb(255,102,102); /* Old browsers */
		/* IE9 SVG, needs conditional override of 'filter' to 'none' */
		background: url(data:image/svg+xml;base64,);
		background: -moz-linear-gradient(45deg,  rgba(255,102,102,1) 0%, rgba(255,204,0,1) 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left bottom, right top, color-stop(0%,rgba(255,102,102,1)), color-stop(100%,rgba(255,204,0,1))); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(45deg,  rgba(255,102,102,1) 0%,rgba(255,204,0,1) 100%); /* Chrome10+,Safari5.1+ */
		background: -o-linear-gradient(45deg,  rgba(255,102,102,1) 0%,rgba(255,204,0,1) 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(45deg,  rgba(255,102,102,1) 0%,rgba(255,204,0,1) 100%); /* IE10+ */
		background: linear-gradient(45deg,  rgba(255,102,102,1) 0%,rgba(255,204,0,1) 100%); /* W3C */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ff6666', endColorstr='#ffcc00',GradientType=1 ); /* IE6-8 fallback on horizontal gradient */
		
		.left-border:after{
			background: #af4646;
		}
	}

	&.purple {
		background: rgb(102,153,204); /* Old browsers */
		/* IE9 SVG, needs conditional override of 'filter' to 'none' */
		background: url(data:image/svg+xml;base64,);
		background: -moz-linear-gradient(45deg,  rgba(102,153,204,1) 0%, rgba(204,102,255,1) 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left bottom, right top, color-stop(0%,rgba(102,153,204,1)), color-stop(100%,rgba(204,102,255,1))); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(45deg,  rgba(102,153,204,1) 0%,rgba(204,102,255,1) 100%); /* Chrome10+,Safari5.1+ */
		background: -o-linear-gradient(45deg,  rgba(102,153,204,1) 0%,rgba(204,102,255,1) 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(45deg,  rgba(102,153,204,1) 0%,rgba(204,102,255,1) 100%); /* IE10+ */
		background: linear-gradient(45deg,  rgba(102,153,204,1) 0%,rgba(204,102,255,1) 100%); /* W3C */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#6699cc', endColorstr='#cc66ff',GradientType=1 ); /* IE6-8 fallback on horizontal gradient */
	
		.left-border:after{
			background: #426588;
		}
	}

	&.seledin {
		background: rgb(51,204,255); /* Old browsers */
		/* IE9 SVG, needs conditional override of 'filter' to 'none' */
		background: url(data:image/svg+xml;base64,);
		background: -moz-linear-gradient(45deg,  rgba(51,204,255,1) 0%, rgba(103,206,82,1) 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left bottom, right top, color-stop(0%,rgba(51,204,255,1)), color-stop(100%,rgba(103,206,82,1))); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(45deg,  rgba(51,204,255,1) 0%,rgba(103,206,82,1) 100%); /* Chrome10+,Safari5.1+ */
		background: -o-linear-gradient(45deg,  rgba(51,204,255,1) 0%,rgba(103,206,82,1) 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(45deg,  rgba(51,204,255,1) 0%,rgba(103,206,82,1) 100%); /* IE10+ */
		background: linear-gradient(45deg,  rgba(51,204,255,1) 0%,rgba(103,206,82,1) 100%); /* W3C */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#33ccff', endColorstr='#67ce52',GradientType=1 ); /* IE6-8 fallback on horizontal gradient */
	
		.left-border:after{
			background: #2280a0;
		}
	}

	&.green {
		background: rgb(0,204,153); /* Old browsers */
		/* IE9 SVG, needs conditional override of 'filter' to 'none' */
		background: url(data:image/svg+xml;base64,);
		background: -moz-linear-gradient(45deg,  rgba(0,204,153,1) 0%, rgba(102,255,255,1) 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left bottom, right top, color-stop(0%,rgba(0,204,153,1)), color-stop(100%,rgba(102,255,255,1))); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(45deg,  rgba(0,204,153,1) 0%,rgba(102,255,255,1) 100%); /* Chrome10+,Safari5.1+ */
		background: -o-linear-gradient(45deg,  rgba(0,204,153,1) 0%,rgba(102,255,255,1) 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(45deg,  rgba(0,204,153,1) 0%,rgba(102,255,255,1) 100%); /* IE10+ */
		background: linear-gradient(45deg,  rgba(0,204,153,1) 0%,rgba(102,255,255,1) 100%); /* W3C */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00cc99', endColorstr='#66ffff',GradientType=1 ); /* IE6-8 fallback on horizontal gradient */
	
		.left-border:after{
			background: #007f5f;
		}
	}

	&.warmPurple {
		background: rgb(255,203,103); /* Old browsers */
		/* IE9 SVG, needs conditional override of 'filter' to 'none' */
		background: url(data:image/svg+xml;base64,);
		background: -moz-linear-gradient(45deg,  rgba(255,203,103,1) 0%, rgba(205,104,202,1) 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left bottom, right top, color-stop(0%,rgba(255,203,103,1)), color-stop(100%,rgba(205,104,202,1))); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(45deg,  rgba(255,203,103,1) 0%,rgba(205,104,202,1) 100%); /* Chrome10+,Safari5.1+ */
		background: -o-linear-gradient(45deg,  rgba(255,203,103,1) 0%,rgba(205,104,202,1) 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(45deg,  rgba(255,203,103,1) 0%,rgba(205,104,202,1) 100%); /* IE10+ */
		background: linear-gradient(45deg,  rgba(255,203,103,1) 0%,rgba(205,104,202,1) 100%); /* W3C */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffcb67', endColorstr='#cd68ca',GradientType=1 ); /* IE6-8 fallback on horizontal gradient */
	
		.left-border:after{
			background: #a58445;
		}
	}

	&.common {
		height: auto;
	}

	&.black {
		.left-border:after {
			background: #000;
		}
	}	

	&.button {
		background: #a4a8b3;

		.content {
			padding-top: 40px;
		}

		.cross {
			display: block;
		}

		.left-border {
			background: #fff;

			&:after {
				background: #ccc;
			}
		}
	}
}
